<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="765" maxWidth="765" prefHeight="765" prefWidth="765" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.jualas.magracontomate_1.scenes.examen_2023.MainAppController">
    <top>
        <Label alignment="TOP_CENTER" prefHeight="45.0" prefWidth="498.0" styleClass="titulo" stylesheets="@main.css" text="Números Primos Gemelos">
         </Label>
    </top>
    <center>
        <TilePane fx:id="PanelCentral" alignment="CENTER" prefColumns="10" stylesheets="@main.css" />
    </center>
    <bottom>
        <HBox alignment="CENTER_RIGHT" spacing="10.0">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         <Button fx:id="btnMenu" mnemonicParsing="false" onAction="#onMenuButtonClick" styleClass="boton-salir" stylesheets="@main.css" text="Menú" />
            <Region fx:id="espaciador" HBox.hgrow="ALWAYS" />
            <Button fx:id="salirButton" onAction="#onActionSalir" prefHeight="35.0" prefWidth="61.0" styleClass="boton-salir" text="Salir">
                <font>
                    <Font size="16.0" />
                </font>
            </Button>
        </HBox>
    </bottom>
</BorderPane>
